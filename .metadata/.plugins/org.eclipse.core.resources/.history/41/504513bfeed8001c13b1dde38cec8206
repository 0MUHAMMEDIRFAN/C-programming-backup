
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int b,a[4][3],x;
	setbuf(stdout,NULL);
	printf("give me numbrs\n");
	for(b=0;b<3;b++){
		for(x=0;x<4;x++){
		scanf("%d",&a[x][b]);
		printf("\t");
		}
		printf("\n");
	}
	for(b=0;b<3;b++){
			for(x=0;x<4;x++)
			{
			printf("\t %d ",a[b][x]);
			}
			printf("\n");
	}
	return EXIT_SUCCESS;
}
