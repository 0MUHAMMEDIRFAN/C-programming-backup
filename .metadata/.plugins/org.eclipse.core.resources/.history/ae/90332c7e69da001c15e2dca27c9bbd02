int ex1()
{
	int a,x,z;
	setbuf(stdout,NULL);
	printf("\n\t enter the size of arrays = ");
	scanf("%d",&a);
	int array1[a],array2[a];
	printf("enter the values of array 1 = ");
	for(x=0;x<a;x++)
	{
		scanf("%d",&array1[x]);
	}
	printf("enter the values of array 2 = ");
	for(x=0;x<a;x++)
		{
			scanf("%d",&array2[x]);
		}
	for(x=0;x<a;x++)
	{
		z=array1[x];
		array1[x]=array2[x];
		array2[x]=z;

	}
	for(x=0;x<a;x++)
		{
			printf("%d  ",array1[x]);
		}
	printf("\n");
	for(x=0;x<a;x++)
			{
				printf("%d  ",array2[x]);
			}
	return EXIT_SUCCESS;
}
