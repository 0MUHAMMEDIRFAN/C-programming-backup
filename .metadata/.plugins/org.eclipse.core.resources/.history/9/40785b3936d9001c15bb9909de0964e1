
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int mark;
	setbuf(stdout,NULL);
	printf("enter your mark\n");
	scanf("%d",&mark);
	printf("\n\t GRADE = ");
	if(mark>90)
		printf("A");
		else if(mark>80)
				printf("B");
		else if(mark>70)
			printf("C");
		else if(mark>60)
			printf("D");
		else if(mark>50)
			printf("E");
		else
			printf("\nstudent is failed");
	return EXIT_SUCCESS;
}
